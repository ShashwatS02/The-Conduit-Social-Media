# backend/.env.example

# --- Application Configuration ---
NODE_ENV=development
PORT=5000
CORS_ORIGIN=http://localhost:5173

# --- Database ---
MONGO_URI=mongodb://localhost:27017/social_dashboard

# --- Authentication ---
JWT_SECRET=your_super_secret_jwt_key_that_is_very_long
JWT_EXPIRES_IN=1d

# --- Cloud Storage Configuration ---
# Set STORAGE_PROVIDER to 'cloudinary' or 's3'
STORAGE_PROVIDER=cloudinary

# --- Cloudinary (if STORAGE_PROVIDER is 'cloudinary') ---
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# --- AWS S3 (if STORAGE_PROVIDER is 's3') ---
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET_NAME=
AWS_REGION=